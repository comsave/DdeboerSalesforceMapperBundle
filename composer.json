{
  "name": "comsave/salesforce-mapper-bundle",
  "type": "symfony-bundle",
  "description": "Salesforce object mapper bundle",
  "homepage": "https://github.com/comsave/salesforce-mapper-bundle",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "LogicItLab\\": "src/LogicItLab"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Tests\\": ""
    }
  },
  "require": {
    "php": ">=7.2.0",
    "comsave/salesforce-bundle": "^3.0.2",
    "symfony/event-dispatcher": "^3.4.9",
    "doctrine/common": "^2.7.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "suggest": {
    "sensio/framework-extra-bundle": "For the param converter"
  },
  "authors": [
    {
      "name": "David de Boer",
      "email": "david@ddeboer.nl"
    },
    {
      "name": "Joeri Veder",
      "email": "joeri.veder@comsave.com"
    },
    {
      "name": "Vaidas Bagdonas",
      "email": "vaidas.bagdonas@comsave.com"
    },
    {
      "name": "Dirk Scholten",
      "email": "dirk.scholten@comsave.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
